<?xml version="1.0"?>
<rolemap>
  <permissions>
    <!-- Map the "add" permissions for the various content types. Note
         that we let the "Contributor" role (in a folder) be allowed
         to add content when the "Owner" is, so that we can
         effectively delegate control over a folder. Some content
         types are considered to be so "structural" that only
         administrators can add them.
      -->
    <permission name="PSJ: Add PSJ Document" acquire="False">
        <role name="Manager" />
    </permission>
  </permissions>
</rolemap>
